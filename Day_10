//***Search in Linked List***//

//**Input:
n = 4
1->2->3->4
Key = 3
Output:
True**//

//**Solution**//


class Solution {
    static boolean searchKey(int n, Node head, int key) {
       
       while(head!=null){
            if(head.data==key)return true;
            head=head.next;
        }
        return false;
    }
}



