287. Find the Duplicate Number ///// https://leetcode.com/problems/find-the-duplicate-number/description/
class Solution {
    public int findDuplicate(int[] nums) { 
        HashSet<Integer>s = new HashSet<>();

        int k = 0;

        for(int i=0;i<nums.length;i++){
            s.add(nums[i]);

            if(s.size() == k){
                return nums[i];
            }
            k = s.size();
        }
        return 0;
    }
}
